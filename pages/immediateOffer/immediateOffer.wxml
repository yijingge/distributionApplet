<view class="immediateOffer_title">
    <wux-row>
        <wux-col span="6" class="immediateOffer_title_left">
            车辆方案
        </wux-col>
        <wux-col span="6" class="immediateOffer_title_right">
            <view class="addOfferButton" hover-class="button_hover">添加车辆方案</view>
        </wux-col>
    </wux-row>
</view>

<view class="card_remarks">
    <view class="card_remarks_title">报价留言</view>
    <view class="card_remarks_content">
        <wux-textarea hasCount rows="4" cursorSpacing="80" maxlength="100" placeholder="请输入留言" />
    </view>
</view>

<view class="immediateOffer_checkBox">
    <checkbox-group class="carKnowledge" bindchange="checkboxChange">
        <label class="checkbox">
            <checkbox value="true" color="#00aaff"/>
        </label>
        <view class="checkbox_text">我已阅读并同意<view class="checkbox_text_special" bindtap="goToCarKnowledge">《用车须知》</view>使用条款</view>
    </checkbox-group>
</view>

<view class="button" hover-class="button_hover">确认报价</view>