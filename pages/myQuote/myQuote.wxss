/* pages/myQuote/myQuote.wxss */
page {
    background-color: #f2f3f4;
}
.page{
  padding:0 30rpx;
}
.priceText {
    text-align: right;
}
.price_red {
    color: #ff4d4d;
}
.tableText {
    display: inline-block;
    width: 20%;
    text-align: center;
    font-size: 28rpx;
}
.rowText {
    font-size: 28rpx;
}
.borderRadiusTopLr{
    border-top-left-radius: 4rpx;
    border-top-right-radius: 4rpx;
}
.borderRadiusBotLr{
    border-bottom-left-radius: 4rpx;
    border-bottom-right-radius: 4rpx;
}
.border-top{
  border-top: 1rpx solid #e8e8e8;
}
.card{
  padding: 30rpx;
}
.card-bottom{
  padding-top: 30rpx;
}
.title {
  padding-bottom: 30rpx;
}