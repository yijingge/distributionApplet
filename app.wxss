/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
.hoverActiveBtn{
  background: #26B7FF;
}
.hoverDefaultBtn{
  background: #D6F1FF;
}
.hoverActiveBtn::after, hoverDefaultBtn::after{
  border: none;
}